{
  "version": 2,
  "builds": [
    {
      "src": "server/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "client/next.config.ts",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/server/(.*)",
      "destination": "/server/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
